{
  "name": "acme",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier -w src",
    "preview": "vite preview",
    "build": "run-s build:*",
    "build:web": "tsc && vite build",
    "build:func": "esbuild src/server/main.ts --bundle --platform=node --external:@azure/functions-core --outfile=functions/dist/main.js",
    "dev": "run-p -l 'dev:*'",
    "dev:vite": "vite",
    "dev:func": "func start -w --prefix functions --cors-credentials",
    "dev:build": "run-s build:func -- --watch",
    "deploy": "swa deploy"
  },
  "dependencies": {
    "@azure/functions": "^4.5.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1",
    "trpc-azure-functions-adapter": "^1.0.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.0"
  }
}
